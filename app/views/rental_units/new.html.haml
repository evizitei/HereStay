.clearfix
- if current_user.vrbo_login.present? && current_user.vrbo_password.present?
  .f_right
    = form_tag load_data_from_vrbo_rental_units_url, :id => 'vrbo_form' do
      =render 'vrbo_form'
  .clearfix
#property_form.property_form
  = error_messages_for @rental_unit
  -#= render :partial => 'upload_photo'
  = form_for @rental_unit, :rental_unit, :url => preview_rental_units_path, :html => { :multipart => true} do |f|
    = render 'form', {:f => f}
= render :partial => 'upload_photo'
  