=content_for :submenu do
  %ul
    %li.current= link_to 'Add Property'
    .clearfix
=property_bread_crumbs(['Create Your Property', 'Publish Your Property', 'Promote Your Property'],1)
.f_right
  = form_tag load_data_from_vrbo_rental_units_url do
    Import From:
    = select_tag 'vrbo_id', options_for_select(current_user.vrbo_listings.map{|vl| [vl[:name], vl[:vrbo_id]]}), :include_blank => 'VRBO', :style => 'width: 60px;'
    = submit_tag 'Go'
.clearfix
#property_form.property_form
  = error_messages_for @rental_unit
  = render :partial => 'upload_photo'
  = form_for @rental_unit, :rental_unit, :url => preview_rental_units_path, :html => { :multipart => true} do |f|
    = render 'form', {:f => f}
  