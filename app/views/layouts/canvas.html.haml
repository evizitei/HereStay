%html
  %head
    = stylesheet_link_tag "jacked_fb_styles"
    %link{:type=>"text/css",:href=>"/jquery-ui/css/redmond/jquery-ui-1.8.2.custom.css",:rel=>"stylesheet"}
    = javascript_include_tag "jquery", "rails"
    %script{:type=>"text/javascript",:src=>"/jquery-ui/js/jquery-ui-1.8.2.custom.min.js"}
    = csrf_meta_tag
  %body
    = link_to image_tag("herestay.png", :border => 0), root_path
    %br
    #fb-root
      = display_flashes
      = yield
    = fb_connect_async_js